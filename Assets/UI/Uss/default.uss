.setting_panel {
    width: 100%;
    height: 100%;
    left: 0;
    top: 100%;
    position: absolute;
    right: 0;
    bottom: 0;
    flex-direction: column;
    background-image: url('project://database/Assets/textures/ui/form.png?fileID=2800000&guid=27c7335a879f5b74fad7437a8abc4ca9&type=3#form');
    justify-content: flex-start;
    align-items: center;
    -unity-font-definition: initial;
    -unity-font: url('project://database/Assets/font/pixelroborobo.otf?fileID=12800000&guid=e515d4b4eb6b7e146a9906f41d96adb0&type=3#pixelroborobo');
    translate: 0 0;
    transition-duration: 0.3s;
    transition-timing-function: ease-in-sine;
}

.setting_panel-up {
    translate: 0 -100%;
    transition-duration: 0.3s;
    transition-timing-function: ease-in-cubic;
    transition-delay: 0s;
}

.setting_base {
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.22);
    display: none;
    transition-property: all;
    transition-duration: 0.5s;
    opacity: 0;
}

.setting_base_fade {
    opacity: 1;
}

