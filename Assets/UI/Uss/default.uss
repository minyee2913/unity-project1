.button_orange {
    align-items: stretch;
    width: 300px;
    height: 120px;
    -unity-font-style: normal;
    font-size: 35px;
    background-image: url('project://database/Assets/textures/ui/button.png?fileID=2800000&guid=3606da6c10316a34cba0533e945e917b&type=3#button');
    -unity-background-scale-mode: stretch-to-fill;
    background-color: rgba(255, 255, 255, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    justify-content: flex-start;
    -unity-background-image-tint-color: rgb(255, 223, 107);
    transition-duration: 0s;
    transition-timing-function: ease;
}

.button_orange:hover {
    scale: 1.05 1.05;
    top: -3px;
    -unity-background-image-tint-color: rgb(255, 239, 182);
    transition-duration: 0.1s;
    transition-timing-function: ease-in-out-elastic;
}

.button_orange:active {
    -unity-background-image-tint-color: rgb(137, 121, 63);
}

